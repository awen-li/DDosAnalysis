LIBIEC_HOME=../..

PROJECT_BINARY_NAME1 = ied1
PROJECT_SOURCES1 = ied1.c

PROJECT_BINARY_NAME2 = ied2
PROJECT_SOURCES2 = ied2.c

include $(LIBIEC_HOME)/make/target_system.mk
include $(LIBIEC_HOME)/make/stack_includes.mk

all:	$(PROJECT_BINARY_NAME1) $(PROJECT_BINARY_NAME2)

include $(LIBIEC_HOME)/make/common_targets.mk

$(PROJECT_BINARY_NAME1):	$(PROJECT_SOURCES1) $(LIB_NAME)
	$(CC) $(CFLAGS) $(LDFLAGS) -o $(PROJECT_BINARY_NAME1) $(PROJECT_SOURCES1) $(INCLUDES) $(LIB_NAME) $(LDLIBS)


$(PROJECT_BINARY_NAME2):  $(PROJECT_SOURCES2) $(LIB_NAME)
	$(CC) $(CFLAGS) $(LDFLAGS) -o $(PROJECT_BINARY_NAME2) $(PROJECT_SOURCES2) $(INCLUDES) $(LIB_NAME) $(LDLIBS)


clean:
	rm -f $(PROJECT_BINARY_NAME1)
	rm -f $(PROJECT_BINARY_NAME2)
